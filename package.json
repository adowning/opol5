{
  "name": "opol5",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "serve": "yarn lint & fuser -k 9000/tcp & fuser -k 8080/tcp & vue-cli-service serve",
    "build": "vue-cli-service build",
    "lint": "vue-cli-service lint",
    "deploy": "./commit_deploy"
  },
  "dependencies": {
    "aws-amplify": "^0.4.1",
    "axios": "^0.18.0",
    "moment": "^2.22.1",
    "register-service-worker": "^1.0.0",
    "vue": "^2.5.16",
    "vue-notification": "^1.3.7",
    "vue-router": "^3.0.1",
    "vuetify": "^1.0.16",
    "vuex": "^3.0.1"
  },
  "devDependencies": {
    "@vue/cli-plugin-babel": "^3.0.0-beta.11",
    "@vue/cli-plugin-eslint": "^3.0.0-beta.11",
    "@vue/cli-plugin-pwa": "^3.0.0-beta.11",
    "@vue/cli-service": "^3.0.0-beta.11",
    "@vue/eslint-config-prettier": "^3.0.0-beta.11",
    "lint-staged": "^6.0.0",
    "vue-cli-plugin-netlify-lambda": "^0.1.0",
    "vue-cli-plugin-vuetify": "^0.1.3",
    "vue-template-compiler": "^2.5.16",
    "node-sass": "^4.7.2",
    "sass-loader": "^6.0.6"
  },
  "eslintConfig": {
    "root": true,
    "env": {
      "node": true
    },
    "extends": [
      "plugin:vue/essential",
      "@vue/prettier"
    ],
    "rules": {
      "no-console": 0,
      "semi": [false, 2]
    },
    "parserOptions": {
      "parser": "babel-eslint"
    }
  },
  "postcss": {
    "plugins": {
      "autoprefixer": {}
    }
  },
  "browserslist": [
    "> 1%",
    "last 2 versions",
    "not ie <= 8"
  ],
  "gitHooks": {
    "pre-commit": "lint-staged"
  },
  "lint-staged": {
    "*.js": [
      "vue-cli-service lint",
      "git add"
    ],
    "*.vue": [
      "vue-cli-service lint",
      "git add"
    ]
  },
  "proxy": {
    "/api": "http://localhost:9000"
  }
}
