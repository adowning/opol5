{
  "version": "0.0.6-snapshot",
  "name": "andrews-admin",
  "description": "Andrews Admin",
  "author": "adowning <ashdowning@gmail.com>",
  "private": false,
  "license": "MIT",
  "main": "electron/index.js",
  "repository": {
    "type": "git",
    "url": "git@github.com:adowning/opol5.git"
  },
  "build": {
    "appId": "com.andrews.app",
    "directories": {
      "buildResources": "static"
    },
    "win": {
      "target": "nsis"
    },
    "linux": {
      "target": "AppImage",
      "category": "Office"
    },
    "publish": {
      "provider": "github",
      "owner": "adowning",
      "repo": "opol5"
    }
  },
  "scripts": {
    "dist": "npx nuxt generate && electron-builder -lw --publish always",
    "lintfix": "eslint --fix --ext .js,.vue --ignore-path .gitignore .",
    "dev": "npx nuxt",
    "build": "npx nuxt generate && electron-builder -lw",
    "start": "nuxt start",
    "generate": "nuxt generate"
  },
  "dependencies": {
    "amazon-cognito-identity-js": "^1.20.0",
    "aws-sdk": "^2.122.0",
    "js-cookie": "^2.1.4",
    "nuxt": "^1.0.0-rc11",
    "vuetify": "^0.14.8",
    "vuex-persistedstate": "^2.0.0"
  },
  "devDependencies": {
    "electron-logger": "^0.0.3",
    "electron-updater": "^2.21.10",
    "babel-eslint": "^7.2.3",
    "eslint": "^4.3.0",
    "eslint-config-standard": "^10.2.1",
    "eslint-loader": "^1.9.0",
    "eslint-plugin-html": "^3.1.1",
    "eslint-plugin-import": "^2.7.0",
    "eslint-plugin-node": "^5.1.1",
    "eslint-plugin-promise": "^3.5.0",
    "eslint-plugin-standard": "^3.0.1",
    "node-sass": "^4.5.3",
    "pug": "^2.0.0-rc.4",
    "sass-loader": "^6.0.6",
    "stylus": "^0.54.5",
    "stylus-loader": "^3.0.1",
    "vuex-router-sync": "^4.3.2"
  }
}
