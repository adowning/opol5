{
    "extends": [
      "eslint:recommended",
      "plugin:vue/recommended"
    ],
    "parserOptions": {
      "ecmaVersion": 2017
  },
    "rules": {
      "import/no-unresolved": 0,
      "import/no-unassigned-import": 0,
      "semi": ["error", "never"],
      "no-console": "off",
      "space-before-function-paren": [
        "warn",
        {
          "anonymous": "always",
          "named": "always",
          "asyncArrow": "always"
        }
      ]
    }
  }